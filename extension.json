{
	"name": "HotCat",
	"author": [
		"Unite together et al."
	],
	"url": "https://www.mediawiki.org/wiki/Extension:HotCat",
	"descriptionmsg": "hotcat-desc",
	"version": "2.43",
	"license-name": "GFDL",
	"type": "editor",
	"MessagesDirs": {
		"HotCat": [
			"i18n"
		]
	},
	"requires": {
		"MediaWiki": ">=1.35"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\HotCat\\": "src/"
	},
	"DefaultUserOptions": {
		"hotcat-switch": 0
	},
	"Hooks": {
		"BeforePageDisplay": "pageDisplay",
		"GetPreferences": "preferences",
		"GetBetaFeaturePreferences": "betaFeature",
		"SaveUserOptions": "preferences"
	},
	"HookHandlers": {
		"preferences": {
			"class": "MediaWiki\\HotCat\\HookHandler\\PreferencesHandler",
			"services": [
				"PermissionManager",
				"UserOptionsLookup",
				"UserGroupManager"
			]
		},
		"betaFeature": {
			"class": "MediaWiki\\HotCat\\HookHandler\\BetaFeaturePreferencesHandler",
			"services": [
				"MainConfig",
				"PermissionManager"
			]
		},
		"pageDisplay": {
			"class": "MediaWiki\\HotCat\\HookHandler\\PageDisplayHandler",
			"services": [
				"PermissionManager",
				"UserOptionsLookup"
			]
		}
	},
	"ResourceModules": {
		"ext.hotcat": {
			"localBasePath": "resources",
			"packageFiles": [
				"ext.hotcat.js"
			]
		}
	},
	"manifest_version": 2
}